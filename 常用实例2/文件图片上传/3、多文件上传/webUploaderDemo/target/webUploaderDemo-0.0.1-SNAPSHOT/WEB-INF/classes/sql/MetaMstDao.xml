<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jp.co.sas.cms.bizshare.dao.MetaMstDao">
	<resultMap type="jp.co.sas.cms.bizshare.dto.MetaMstDto" id="MetaMstDtoBean">
		<result column="meta_mst_id" property="metaMstId" />
		<result column="meta_mst_name" property="metaMstName" />
		<result column="comment" property="comment" />
		<result column="update_user_id" property="updateUserId" />
		<result column="create_datetime" property="createDatetime" />
		<result column="update_datetime" property="updateDatetime" />
	</resultMap>
	<update id="updateMetaMst" parameterType="jp.co.sas.cms.bizshare.dto.MetaMstDto">
		<![CDATA[
			update aa_meta_mst  set comment=#{comment},meta_mst_name=#{metaMstName}	
			]]>
		<![CDATA[
			where meta_mst_id=#{metaMstId}
		]]>
	</update>
	<select id="selectAaMetaMstById" resultMap="MetaMstDtoBean"
		parameterType="String">
		SELECT *
		FROM aa_meta_mst
		WHERE
		trim(meta_mst_id)=#{metaMstId}
	</select>
	<select id="selectAaMetaMst" resultMap="MetaMstDtoBean"
		parameterType="String">
		SELECT *
		FROM aa_meta_mst
		WHERE
		trim(meta_mst_name)=#{metaMstName}
	</select>
	<delete id="deletemMetaMst" parameterType="String">
		DELETE FROM
		aa_meta_mst
		WHERE meta_mst_id=#{metaMstId}
	</delete>
	<select id="MetaMstList" resultType="jp.co.sas.cms.bizshare.dto.MetaMstDto">
		SELECT meta_mst_id
		metaMstId,meta_mst_name metaMstName, comment comment,update_user_id
		updateUserId,create_datetime createDatetime,update_datetime
		updateDatetime
		FROM aa_meta_mst   where   1=1     order by    meta_mst_name  asc
	</select>
	<insert id="aaMetaMstRegister" parameterType="jp.co.sas.cms.bizshare.dto.MetaMstDto">
		INSERT INTO aa_meta_mst
		(meta_mst_id
		,meta_mst_name
		,comment
		,update_user_id
		,create_datetime
		,update_datetime
		)
		VALUES
		(
		#{metaMstId},
		#{metaMstName},
		#{comment},
		#{updateUserId},
		#{createDatetime},
		#{updateDatetime}
		)
	</insert>
</mapper>