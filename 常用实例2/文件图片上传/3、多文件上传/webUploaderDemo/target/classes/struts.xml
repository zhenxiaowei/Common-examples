<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<bean type="com.opensymphony.xwork2.TextProvider" name="default" class="jp.co.sas.cms.web.message.SystemTextProvider" scope="default" /> 
	<constant name="struts.objectFactory" value="spring" />
	<constant name="struts.objectFactory.spring.useClassCache" value="true" />
	<constant name="struts.freemarker.templatesCache" value="true" />
	<constant name="struts.freemarker.beanwrapperCache" value="true" />
	<constant name="struts.enable.DynamicMethodInvocation" value="true" />
	<constant name="struts.devMode" value="true" />
	<constant name="struts.server.static.browserCache" value="false" />
	<constant name="struts.ui.theme" value="simple" />
	<constant name="struts.locale" value="jp" />
	<constant name="struts.i18n.encoding" value="UTF-8" />
	<constant name="struts.custom.i18n.resources" value="message" />
	<constant name="struts.multipart.maxSize" value="99999999999999" />
	<constant name="struts.convention.result.path" value="/" />
	<constant name="struts.convention.package.locators.basePackage" value="webUploaderDemo.webUploaderDemo.action" />
	<constant name="struts.convention.default.parent.package" value="web-package" />
	<constant name="struts.convention.action.name.lowercase" value="false" />
	<constant name="struts.convention.action.name.separator" value="" />
	<constant name="struts.convention.relative.result.types" value="velocity" />
	<package name="web-package" extends="struts-default" abstract="true">
		<result-types>
			<result-type name="velocity" class="jp.co.sas.cms.web.resulttype.CmsVelocityResult" />
			<!-- <result-type name="redirect" class="jp.co.sas.cms.web.resulttype.CmsServletActionRedirectResult"/> -->
		</result-types>
		<interceptors>
			<interceptor-stack name="sysStack">
			   <interceptor-ref name="defaultStack" />
			</interceptor-stack>
		</interceptors>
		<default-interceptor-ref name="sysStack"></default-interceptor-ref>
		<global-results>
			<result name="SysError" type="dispatcher">/common/error.jsp</result>
		</global-results>
	</package>
</struts>
