<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//Apache Struts//XWork Validator 1.0.3//EN"  "http://struts.apache.org/dtds/xwork-validator-1.0.3.dtd">
<validators>
	<field name="audio.audioClientName">
		<field-validator type="requiredstring">
			<message key="W00000001">
				<param name="0">'Audio編集クライアント名 '</param>
			</message>
		</field-validator>
	</field>

	<field name="audio.fileType">
		<field-validator type="requiredstring">
			<message key="W00000001">
				<param name="0">'音源ファイルFileType名 ' </param>
			</message>
		</field-validator>
	</field>
	<field name="audio.audioFileExtension">
		<field-validator type="requiredstring">
			<message key="W00000001">
				<param name="0">'音源ファイル拡張子 ' </param>
			</message>
		</field-validator>
	</field>
	<field name="audio.preFileExtension">
		<field-validator type="requiredstring">
			<message key="W00000001">
				<param name="0">'プレビュー元ファイル拡張子 ' </param>
			</message>
		</field-validator>
	</field>
	<field name="audio.ipAddress">
		<field-validator type="requiredstring">
			<message key="W00000001">
				<param name="0">'Audio編集クライアントIPアドレス ' </param>
			</message>
		</field-validator>
	</field>
	<field name="audio.folder">
		<field-validator type="requiredstring">
			<message key="W00000001">
				<param name="0">'音声データ格納共有フォルダ名 ' </param>
			</message>
		</field-validator>
	</field>
	<field name="audio.password">
		<field-validator type="requiredstring">
			<message key="W00000001">
				<param name="0">'新しいパスワード ' </param>
			</message>
		</field-validator>
	</field>
	<field name="audio.updateUserId">
		<field-validator type="requiredstring">
			<message key="W00000001">
				<param name="0">'パスワード確認 ' </param>
			</message>
		</field-validator>
	</field>
</validators> 
